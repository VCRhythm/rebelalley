<h1>Listing survey_results</h1>

<table>
  <tr>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @survey_results.each do |survey_result| %>
  <tr>
    <td><%= survey_result.content %></td>
    <td><%= link_to 'Show', survey_result %></td>
    <td><%= link_to 'Edit', edit_survey_result_path(survey_result) %></td>
    <td><%= link_to 'Destroy', survey_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

